{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "@now/python@1.1.6",
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "api$1"
    },
    {
      "src": "/_next(.*)",
      "dest": "frontend/_next$1"
    },
    {
      "src": "(.*)",
      "dest": "frontend$1"
    }
  ]
}
