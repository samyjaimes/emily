{
  "version": 2,
  "functions": {
    "backend/api.py": {
      "runtime": "@now/python",
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/backend",
      "dest": "api$1"
    },
    {
      "src": "/_next(.*)",
      "dest": "frontend/_next$1"
    },
    {
      "src": "(.*)",
      "dest": "frontend$1"
    }
  ]
}
