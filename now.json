{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@now/next"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "@now/python",
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "api$1"
    },
    {
      "src": "/_next(.*)",
      "dest": "frontend/_next$1"
    },
    {
      "src": "(.*)",
      "dest": "frontend$1"
    }
  ]
}
